 .icon_class:hover
  {
      transform: translate(1px, 1px);   
      cursor:pointer;   
  }

  .icon_class:active
  {
      transform: translate(1px, 1px);
  }

 
  .edit_icon
  { 
    display: inline-block;    
    background: url(../imgs/edit.png) no-repeat;
    height: 36px;
    width: 40px;  
    background-position: 7px 6px;
    vertical-align: middle; 
  }
  .export_icon
  { 
    display: inline-block;    
    background: url(../imgs/download.png) no-repeat;
    height: 36px;
    width: 40px;  
    background-position: 7px 6px;
    vertical-align: middle; 
  }
  
  .icon_class
  {
    display: inline-block;  
//    height: 100%;
    vertical-align: middle;
    letter-spacing:2px;
    font-weight:bold;

