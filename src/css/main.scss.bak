.tool_header
{
	width: 100% ;
	height: $toolbar_hight;
	background: $tool_header_color;
	line-height:$toolbar_hight;
	position:absolute;
	top: 0px;
    left:0px;
	
	
//	display: flex;
//	justify-content:space-between;
//	 flex-direction:column ;
	 

}



#icon_container_left{
  height:$toolbar_hight;
  margin-left:20px;
  display:block;
   float: left;
  
}
#icon_container_right{
  display: block;
  height:$toolbar_hight;
  float: right;
  
}



#display_area
{
	position: absolute;
	top:$toolbar_hight;
	left:0;
	width: 100% ;
	height:869px;
	border-bottom: 1px solid rgb(149, 155, 111);
	border-top: 1px solid rgb(149, 155, 111);
}

#container_foot
{
	position: absolute;
	top:0;
	left:0;
	width: 100% ;
	height:$container_foot_hight;
	background: $tool_header_color;

}

#editor
{
	position: absolute;
	top:0;
	left:0;
	width: 300px;//50% ;
	height:100%;
	margin:0 auto;	
	padding: 20px;    
	overflow: auto;//scroll;
	background-color: $editor_bg_color;
  font-family: courier, monospace;
  font-size: inherit;
  color: $edit_font_color;
  
  //会自动换行，当到框边时就自动换行。
  word-wrap: break-word;
  word-break: normal;	 
  white-space: pre-wrap;

/*
 // 不会自动换行，除非你enter换行
  word-wrap: normal !important;
  white-space: normal !important;
	white-space: pre !important;
*/	
	
}

#drag_indicate
{
	position: absolute;
	top:0;
	left:0px;
	transform: translate(300px, 0px);
	width:10px;
	height:100%;	
	user-select: none;
	user-drag: none;
	touch-acion:pan-y;
	cursor:e-resize;
	background-color: #f6f6f6;
	border-left: 1px solid rgb(149, 155, 111);
	border-right: 1px solid rgb(149, 155, 111);
  z-index: 20;
}

#editor_render
{
	position: absolute;
	top:0;
	left:0px;
	transform: translate(310px, 0px);
	width: 300px;
	height:100%;
	margin:0 auto;
	padding: 20px;
	overflow: auto;//scroll;
	background-color: #fff;	
}



